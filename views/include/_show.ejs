<% for(let i= 0;i< crudapp.length;i++){%>
<tr>
  <td><%= i + 1%></td>
  <td><%= crudapp[i].name%></td>
  <td><%= crudapp[i].email%></td>
  <td><%= crudapp[i].gender%></td>
  <td><%= crudapp[i].status%></td>
  <td>
    <a
      href="/update-user?id=<%= crudapp[i]._id%>"
      class="btn border-shadow update"
    >
      <span class="text-gradient"><i class="fa-solid fa-pencil"></i></span>
    </a>
    <a class="btn border-shadow delete" data-id="<%= crudapp[i]._id%>">
      <span class="text-gradient"><i class="fa-solid fa-xmark"></i></span>
    </a>
  </td>
</tr>
<%}%>
